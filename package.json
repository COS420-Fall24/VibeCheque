  "homepage": "https://github.com/COS420-Fall24/VibeCheque#readme",
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "firebase": "^11.5.0",
    "openai": "^4.68.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
